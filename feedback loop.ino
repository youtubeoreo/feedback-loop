void setup()
{
  pinMode(13,OUTPUT);
  pinMode(9,OUTPUT);
  pinMode(3,OUTPUT);
}
void loop()
{
  if(digitalRead(13)==HIGH)
  {
    digitalRead(9,LOW);
    digitalRead(3,LOW);
  }
  else if((digitalRead(13)==LOW)
  {
    digitalRead(9,HIGH);
    digitalRead(3,LOW);
  }
  else if ((digitalRead(9)==LOW)
  {
  digitalRead(3,HIGH);
  digitalRead(13,LOW);
}
   else((digitalRead(3)==LOW) 
   {
     digitalRead(13,HIGH);
     digitalRead(9,LOW);
   }
   
